{
  "name": "@electron-mcp/workspace",
  "version": "1.0.0",
  "description": "Electron MCP monorepo with Yarn workspaces",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "yarn workspace @electron-mcp/electron-app start",
    "dev": "yarn workspace @electron-mcp/electron-app dev",
    "rpc": "yarn workspace @electron-mcp/rpc-server start",
    "build": "yarn workspaces run build",
    "test": "yarn workspaces run test",
    "test:watch": "yarn workspaces run test:watch",
    "format": "yarn workspaces run format",
    "format:check": "yarn workspaces run format:check",
    "clean": "yarn workspaces run clean && rm -rf node_modules"
  },
  "keywords": ["electron", "mcp", "monorepo", "workspace"],
  "author": "",
  "license": "MIT",
  "dependencies": {

  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "prettier": "^3.0.0",
    "supertest": "^7.2.2"
  }
}
